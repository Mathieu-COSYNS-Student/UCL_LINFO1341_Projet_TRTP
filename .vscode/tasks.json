{
  "version": "2.0.0",
  "command": "bash",
  "options": {
    "cwd": "${workspaceRoot}"
  },
  "args": ["-c"],
  "tasks": [
    {
      "label": "make debug",
      "type": "shell",
      "command": "make",
      "args": ["debug"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "make test",
      "type": "shell",
      "command": "make",
      "args": ["test"],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}
